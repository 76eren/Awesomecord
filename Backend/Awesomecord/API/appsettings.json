{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "${CONNECTIONSTRINGS__DEFAULTCONNECTION}"
  },
  "Jwt": {
    "Issuer": "${JWT_ISSUER}",
    "Audience": "${JWT_AUDIENCE}",
    "SigningKey": "${JWT_SIGNING_KEY}",
    "AccessTokenMinutes": "${JWT_ACCESS_TOKEN_MINUTES}",
    "RefreshTokenDays": "${JWT_REFRESH_TOKEN_DAYS}"
  },
  "Minio": {
    "Endpoint": "${MINIO_ENDPOINT}",
    "AccessKey": "${MINIO_ACCESS_KEY}",
    "SecretKey": "${MINIO_SECRET_KEY}",
    "Bucket": "${MINIO_BUCKET}",
    "WithSsl": "${MINIO_WITH_SSL}",
    "PreSignedExpiryMinutes": "${MINIO_PRESIGNED_EXPIRY_MINUTES}"
  },
  "FrontendOrigins": "${FRONTEND_ORIGINS}"
}
